{
   "selector": {
      "createdDate": {
         "$gte": "2021-08-23T12:04:12Z"
      }
   }
}


selector with OR and docType Not Equal to condition in @and parameter


queryString := `{
	   "selector": {
		   "$and": [
			   {
				  "$or": [
					 {
						"productID":"` + productId + `"
					 },
					 {
						"productId": "` + productId + `"
					 }
				  ]
			   },
			   {
				  "docType": {
					 "$ne": "PRODUCT"
				  }
			   }
			]
		 }
	}`
	
	
	
Nested Column Index

data.Input.MLT_SYSTEM_ID

{
   "selector": {
      "assetType": "Inventory",
      "docType": "Asset",
      "data": {
         "Input": {
            "$elemMatch": {
               "MTL_SYSTEM_ID": {
                  "$in": [
                     "100",
                     "101"
                  ]
               }
            }
         }
      }
   }
}	
